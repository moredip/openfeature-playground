{
  "name": "demo-express-weather-app",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/demo-express-weather-app/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/apps/demo-express-weather-app",
        "main": "apps/demo-express-weather-app/src/main.js",
        "tsConfig": "apps/demo-express-weather-app/tsconfig.app.json",
        "assets": [
          "apps/demo-express-weather-app/src/assets",
          "apps/demo-express-weather-app/src/views"
        ]
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false
        }
      }
    },
    "serve": {
      "executor": "@nrwl/js:node",
      "options": {
        "buildTarget": "demo-express-weather-app:build"
      },
      "configurations": {
        "production": {
          "buildTarget": "demo-express-weather-app:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/demo-express-weather-app/**/*.js"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/demo-express-weather-app/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
